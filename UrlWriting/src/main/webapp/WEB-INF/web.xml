<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>servlet1</servlet-name>
		<servlet-class>com.urlRewrite.Servlet1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>servlet1</servlet-name>
		<url-pattern>/servlet1</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ServletTwo</servlet-name>
		<servlet-class>com.urlRewrite.ServletTwo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletTwo</servlet-name>
		<url-pattern>/ServletTwo</url-pattern>
	</servlet-mapping>

</web-app>
