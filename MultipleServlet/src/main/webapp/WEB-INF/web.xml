<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<welcome-file-list>
<welcome-file>index.html</welcome-file>
</welcome-file-list>

<servlet>
<servlet-name>servlet1</servlet-name>
<servlet-class>com.servletMultiple.Servlet1</servlet-class>
</servlet>

<servlet>
<servlet-name>servlet2</servlet-name>
<servlet-class>com.servletMultiple.Servlet2</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>servlet1</servlet-name>
<url-pattern>/submit</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>servlet2</servlet-name>
<url-pattern>/servlet2</url-pattern>
</servlet-mapping>
</web-app>